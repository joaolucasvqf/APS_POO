      </MenuBar>
      <TableView fx:id="tvResult" layoutX="61.0" layoutY="43.0" prefHeight="281.0" prefWidth="479.0">
        <columns>
          <TableColumn fx:id="tcPessoas" prefWidth="49.0" text="Pessoas" />
          <TableColumn fx:id="tcPratos" prefWidth="44.0" text="Pratos" />
            <TableColumn fx:id="tcSalada" prefWidth="42.0" text="Salada" />
            <TableColumn fx:id="tcCarboidrato" minWidth="3.0" prefWidth="73.0" text="Carboidrato" />
