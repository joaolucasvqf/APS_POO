            <TableColumn fx:id="tcMedio" prefWidth="92.0" text="Tempo Médio" />
        </columns>
      </TableView>
      <Button fx:id="btnVoltar" layoutX="426.0" layoutY="342.0" mnemonicParsing="false" onAction="#clickVoltar" text="Voltar à tela inicial" />
